    <div class="addToPartyScreens">

      <div class="addBookToPartyScreen mainAddScreen">
        <div class="addToParty-formBox addBookToParty">
          <div class="tag" id="bookPartyNameTag"></div>
          <div class="tag" id="addBookNameTag"></div>
          <form class="form" id="addBookToPartyForm">

            <div class="inputs">

              <label for="bookToPartyAmountInput">Cantidad: <i>(requerido)</i></label>
              <input type="number" name="prodAmount" id="bookToPartyAmountInput" required>

              <label for="bookToPartyNewpriceInput">Precio para este evento: <i>(opcional)</i></label>
              <input type="number" name="prodAmount" id="bookToPartyNewpriceInput" pattern="\d+(\.\d{1,2})?">

            </div>

            <div class="btns">
              <button type="button" class="cancel" id="cancelAddBookToPartyBtn">No agregar a este evento</button>

              <button type="submit" class="accept" id="AcceptAddBookToPartyBtn">Continuar</button>
            </div>
          </form>
        </div>

      </div>

      <div class="addSeparToPartyScreen mainAddScreen">
        <div class="addToParty-formBox addSeparToParty">
          <div class="tag" id="separPartyNameTag"></div>
          <div class="tag" id="addSeparNameTag"></div>
          <form class="form" id="addSeparToPartyForm">

            <div class="inputs">

              <label for="separToPartyAmountInput">Cantidad: <i>(requerido)</i></label>
              <input type="number" name="prodAmount" id="separToPartyAmountInput" required>

              <label for="separToPartyNewpriceInput">Precio para este evento: <i>(opcional)</i></label>
