    <div class="articleBox discounts-box">
      <div class="row one">
        <div class="box-name">
          <p class="text-name">Descuentos</p>
        </div>
        <div class="box-menu">
          <div class="win-menu">

            <div class="buttonBar-box">
              <div class="discount-btn addBtn" id="addDiscountBtn">+ Añadir descuento</div>
            </div>
          </div>
        </div>

      </div>
      <div class="row two discounts-screen">
